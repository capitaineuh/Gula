    "next": "14.0.3",
    "react": "18.2.0",
    "react-chartjs-2": "5.2.0",
    "react-dom": "18.2.0",
    "three": "0.159.0"
  },
  "devDependencies": {
    "@types/node": "20.10.4",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "@types/three": "0.159.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.54.0",
